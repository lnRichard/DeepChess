{
  "name": "deepchess",
  "version": "1.0.3",
  "description": "A minimal Electron chess application",
  "private": true,
  "main": "main.js",
  "build": {
    "appId": "deepchess.chess",
    "mac": {
      "category": "chess"
    }
  },
  "scripts": {
    "start": "electron .",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder"
  },
  "keywords": [
    "electron",
    "chess",
    "minimal",
    "application",
    "ai"
  ],
  "author": "Veldhoven van, Richard Et al.",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^16.0.2",
    "electron-builder": "^22.14.13",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.4.1",
    "eslint-plugin-sonarjs": "^0.12.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "yarn": "^1.22.17"
  },
  "dependencies": {
    "@electron/remote": "^2.0.7",
    "chess.js": "^0.12.0",
    "jquery": "^3.6.0",
    "stockfish": "^14.1.0"
  }
}
